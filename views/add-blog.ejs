<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <%- include('./partials/header') %>
</head>
<body>
  <%- include("./partials/nav") %>
  <form class="mx-5"method="post" action="/blog" enctype="multipart/form-data">
    <div class="mt-3">
      <label for="formFile" class="form-label">Cover Image</label>
      <input class="form-control" type="file" id="formFile" name="cover-image">
    </div>
    <div class="mt-3">
      <label for="exampleInputTitle" class="form-label">Title</label>
      <input name="title" type="Title" class="form-control" id="exampleInputTitle" aria-describedby="titleHelp">
    </div>
    <div class="mt-3">
      <label class="form-label">Body</label>
      <textarea class="form-control" aria-label="With textarea" name="body"></textarea>
    </div>
    
    <div class="mt-3">
    <button type="submit" class="btn btn-primary">Submit</button>
  </div>
  </form>
   <%- include('./partials/script') %>

</body>
</html>